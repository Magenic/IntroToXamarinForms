<?xml version="1.0" encoding="UTF-8" ?>
<FlyoutPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="IntroToXamarin.MyFlyoutPage"
    xmlns:layouts="clr-namespace:IntroToXamarin">
    <FlyoutPage.Flyout>
        <layouts:MyFlyout x:Name="flyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <layouts:PageA />
            </x:Arguments>
     </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>
